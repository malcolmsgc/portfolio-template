{
  "name": "demo-portfolio",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sasquatch": "node-sass --watch src -o src",
    "sass": "node-sass src -o src",
    "dvlp": "concurrently \"yarn sasquatch\" \"yarn start\""
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "node-sass": "4.5.3",
    "react-icons-kit": "^1.0.7"
  }
}
